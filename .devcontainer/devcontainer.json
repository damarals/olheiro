{
	"name": "Olheiro",
	"build": {
        "dockerfile": "Dockerfile"
    },
	"workspaceMount": "source=${localWorkspaceFolder},target=/home/olheiro/app,type=bind",
	"workspaceFolder": "/home/olheiro/app",
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {},
		"ghcr.io/stuartleeks/dev-container-features/shell-history:0": {},
		"ghcr.io/nils-geistmann/devcontainers-features/zsh:0": {}
	},
	"remoteUser": "olheiro",
	"containerUser": "olheiro",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	}
}